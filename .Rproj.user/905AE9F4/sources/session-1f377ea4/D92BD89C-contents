# URL of the WHO Disease Outbreak News url
url <- "https://www.who.int/emergencies/disease-outbreak-news"




# Extraer los tÃ­tulos completos
full_titles <- read_html(url) %>%
  html_nodes("h4.sf-list-vertical__title .full-title") %>%
  html_text(trim = TRUE)

# Extraer las fechas
fechas <- read_html(url) %>%
  html_nodes("h4.sf-list-vertical__title span:nth-child(2)") %>%
  html_text(trim = TRUE)

# Extraer los enlaces (href)
enlaces <- read_html(url) %>%
  html_nodes("a.sf-list-vertical__item") %>%
  html_attr("href")
